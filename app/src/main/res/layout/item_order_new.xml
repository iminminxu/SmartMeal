<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/dp_13"
    android:layout_marginTop="@dimen/dp_13"
    android:layout_marginEnd="@dimen/dp_13"
    android:background="@drawable/corners_3_solid_white">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.222" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.567" />

    <View
        android:id="@+id/view_bg"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_38"
        android:background="@drawable/corners_3_3_0_0_solid_0aec7220"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_order_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_13"
        android:layout_marginEnd="@dimen/dp_13"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/font_13"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toBottomOf="@id/view_bg"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintLeft_toLeftOf="@id/view_bg"
        app:layout_constraintRight_toLeftOf="@id/tv_order_time"
        app:layout_constraintTop_toTopOf="@id/view_bg"
        tools:text="订单编号：202211231029252840" />

    <TextView
        android:id="@+id/tv_order_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_63"
        android:layout_marginEnd="@dimen/dp_63"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/font_13"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toBottomOf="@id/view_bg"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintLeft_toLeftOf="@id/guideline_one"
        app:layout_constraintRight_toLeftOf="@id/tv_order_type"
        app:layout_constraintTop_toTopOf="@id/view_bg"
        tools:text="下单时间：2022-09-09  08:23:19" />

    <TextView
        android:id="@+id/tv_order_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_13"
        android:textColor="@color/color_ec7220"
        android:textSize="@dimen/font_12"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/view_bg"
        app:layout_constraintRight_toRightOf="@id/view_bg"
        app:layout_constraintTop_toTopOf="@id/view_bg"
        tools:text="华数TV" />

    <TextView
        android:id="@+id/tv_commodity_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_15"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/font_15"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@id/tv_order_no"
        app:layout_constraintTop_toBottomOf="@id/view_bg"
        tools:text="商品（2）" />

    <TextView
        android:id="@+id/tv_commodity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_13"
        android:layout_marginBottom="@dimen/dp_19"
        android:lineSpacingExtra="@dimen/dp_5"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/font_13"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintLeft_toLeftOf="@id/tv_order_no"
        app:layout_constraintRight_toLeftOf="@id/guideline_one"
        app:layout_constraintTop_toBottomOf="@id/tv_commodity_title"
        app:layout_constraintVertical_bias="0"
        tools:text="一荤一素\n两荤一素" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/font_15"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/tv_mobile"
        app:layout_constraintLeft_toLeftOf="@id/tv_order_time"
        app:layout_constraintTop_toTopOf="@id/tv_mobile"
        tools:text="王梦阳" />

    <TextView
        android:id="@+id/tv_mobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_9"
        android:layout_marginTop="@dimen/dp_20"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/font_13"
        app:layout_constraintLeft_toRightOf="@id/tv_name"
        app:layout_constraintTop_toBottomOf="@id/view_bg"
        app:layout_goneMarginStart="0dp"
        tools:text="1827647382" />

    <TextView
        android:id="@+id/tv_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_11"
        android:layout_marginEnd="@dimen/dp_56"
        android:lineSpacingExtra="@dimen/dp_5"
        android:textColor="@color/color_777777"
        android:textSize="@dimen/font_13"
        app:layout_constrainedWidth="true"
        app:layout_constraintBottom_toTopOf="@id/tv_improve_info"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintLeft_toLeftOf="@id/tv_order_time"
        app:layout_constraintRight_toLeftOf="@id/guideline_two"
        app:layout_constraintTop_toBottomOf="@id/tv_mobile"
        app:layout_constraintVertical_bias="0"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_goneMarginBottom="@dimen/dp_21"
        tools:text="杭州市西湖区北山街道上保社区松木床河西28-2-2" />

    <TextView
        android:id="@+id/tv_improve_info"
        android:layout_width="@dimen/dp_101"
        android:layout_height="@dimen/dp_30"
        android:layout_marginTop="@dimen/dp_11"
        android:layout_marginBottom="@dimen/dp_27"
        android:background="@drawable/corners_3_solid_ec7220"
        android:gravity="center"
        android:text="完善老人信息"
        android:textColor="@color/white"
        android:textSize="@dimen/font_13"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/tv_order_time"
        app:layout_constraintTop_toBottomOf="@id/tv_address"
        app:layout_constraintVertical_bias="0" />

    <TextView
        android:id="@+id/tv_income"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_64"
        android:textColor="@color/color_f71413"
        android:textSize="@dimen/font_16"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/guideline_two"
        app:layout_constraintTop_toBottomOf="@id/view_bg"
        tools:text="预估收入：¥53.00" />

    <TextView
        android:id="@+id/tv_operate"
        android:layout_width="@dimen/dp_101"
        android:layout_height="@dimen/dp_30"
        android:layout_marginEnd="@dimen/dp_13"
        android:background="@drawable/corners_3_solid_ec7220"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/font_13"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_bg"
        tools:text="立即接单" />

    <View
        android:layout_width="@dimen/dp_1"
        android:layout_height="@dimen/dp_63"
        android:background="@color/color_f0f0f0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/guideline_one"
        app:layout_constraintRight_toRightOf="@id/guideline_one"
        app:layout_constraintTop_toBottomOf="@id/view_bg" />

    <View
        android:layout_width="@dimen/dp_1"
        android:layout_height="@dimen/dp_63"
        android:background="@color/color_f0f0f0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/guideline_two"
        app:layout_constraintRight_toRightOf="@id/guideline_two"
        app:layout_constraintTop_toBottomOf="@id/view_bg" />
</androidx.constraintlayout.widget.ConstraintLayout>